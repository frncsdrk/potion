{
  "name": "renaissance",
  "version": "0.5.22",
  "description": "renovating js components",
  "main": "dist/renaissance.js",
  "scripts": {
    "browserify:demo": "browserify -t babelify demo/src/app.js -o demo/dist/app.js",
    "uglify:demo": "uglifyjs demo/dist/app.js -o demo/dist/app.min.js --compress --mangle --source-map",
    "babel": "babel -d dist/ src/",
    "minify": "minify dist/renaissance.js --out-file dist/renaissance.min.js",
    "build": "npm run babel",
    "build:demo": "npm run build && npm run browserify:demo && npm run uglify:demo",
    "dev": "nodemon --ignore dist/ --ignore demo/ --ignore test/",
    "demo": "nodemon --ignore dist/ --ignore demo/dist/",
    "test": "tape -r babel-register test/**/*.js | faucet",
    "test:nyc": "nyc npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frncsdrk/renaissance.git"
  },
  "keywords": [
    "renaissance",
    "component",
    "js"
  ],
  "author": "frncsdrk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/frncsdrk/renaissance/issues"
  },
  "homepage": "https://github.com/frncsdrk/renaissance#readme",
  "dependencies": {
    "babel-core": "^6.26.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-minify": "^0.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2017": "^6.24.1",
    "babelify": "^10.0.0",
    "browserify": "^16.1.1",
    "faucet": "^0.0.1",
    "nodemon": "^1.18.6",
    "nyc": "^13.3.0",
    "tape": "^4.9.0",
    "uglify-js": "^3.4.9"
  }
}
